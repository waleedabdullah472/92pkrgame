{
    "version": 2,
    "builds": [
      {
        "src": "pages/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/about",
        "dest": "/pages/about.js"
      },
      {
        "src": "/privacy",
        "dest": "/pages/privacy.js"
      },
      {
        "src": "/contact",
        "dest": "/pages/contact.js"
      }
    ]
  }
  